<AnchorPane id="NewMember" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.NewMemberController">
    <children>
        <VBox layoutX="150.0" layoutY="256.0" prefHeight="338.0" prefWidth="168.0" AnchorPane.leftAnchor="204.0" AnchorPane.topAnchor="256.0">
            <children>
            <Label id="label" text="Ime" textFill="#d3190f" />
            <TextField fx:id="tfName" />
            <Label id="label" text="Prezime" textFill="#d3190f" />
            <TextField fx:id="tfPrename" />
            <Label text="Tarifa" textFill="#d3190f" />
            <ChoiceBox fx:id="cb" prefWidth="150.0" />
            <Button fx:id="save" mnemonicParsing="false" onAction="#saveMember" text="Sačuvaj">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="50.0" />
               </VBox.margin>
            </Button>
                
            </children>
        </VBox>
      <VBox layoutX="20" layoutY="40" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="bback" minHeight="50.0" minWidth="100.0" onAction="#back" text="Nazad">
               <font>
                  <Font name="Arial" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
    </children>
</AnchorPane>
